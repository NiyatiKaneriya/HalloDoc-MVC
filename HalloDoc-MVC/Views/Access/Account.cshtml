@{
}
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<section class="container-fluid w-75 my-5">
    <h3>Account Access</h3>
    <div class="d-flex flex-row-reverse">
        <a asp-action="CreateAccess" asp-controller="Access" class="btn btn-outline-info   ">Create Access</a>
    </div>
    
    <div class="table" id="Accessdata-table">
    </div>
    
</section>
@section Scripts{


    <script>
        $(document).ready(function () {
            $.ajax({
                url: '/Access/GetAccounts',
                type: 'GET',
                data : '',
                success: function (data) {
                    $('#Accessdata-table').html(data);

                },
                error: function (xhr, status, error) {
                    console.error("Error occurred: " + error);
                }
            });
        });
        
    </script>
}